{
  "name": "electron-ts",
  "version": "1.0.0",
  "description": "An electron application written with Typescript",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.backend.json && webpack --mode=production",
    "start": "electron ./dist/backend/main.js",
    "start:dev:backend": "tsc --watch --project tsconfig.backend.json",
    "start:dev:frontend": "webpack --watch --mode=development"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "electron": "^15.2.0",
    "electron-reloader": "^1.2.1",
    "eslint": "^7.32.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "electron-reload": "^2.0.0-alpha.1"
  }
}
